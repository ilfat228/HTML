import random
import string

def generate_passwords(full_name, birth_date, num_passwords=10):
    name_parts = full_name.replace(" ", "").lower()  # –£–±–∏—Ä–∞–µ–º –ø—Ä–æ–±–µ–ª—ã –∏ –¥–µ–ª–∞–µ–º —Å—Ç—Ä–æ—á–Ω—ã–µ –±—É–∫–≤—ã
    birth_parts = birth_date.replace(".", "").replace("-", "")  # –£–±–∏—Ä–∞–µ–º —Ç–æ—á–∫–∏ –∏ –¥–µ—Ñ–∏—Å—ã
    
    base_words = [
        name_parts, birth_parts, name_parts[::-1], birth_parts[::-1],  # –û–±—ã—á–Ω—ã–µ –∏ –ø–µ—Ä–µ–≤—ë—Ä–Ω—É—Ç—ã–µ
        name_parts[:3] + birth_parts[-2:], birth_parts[:4] + name_parts[-2:]
    ]
    
    special_chars = "@#$%&*!"
    passwords = []

    for _ in range(num_passwords):
        base = random.choice(base_words)  # –í—ã–±–∏—Ä–∞–µ–º –æ—Å–Ω–æ–≤—É –¥–ª—è –ø–∞—Ä–æ–ª—è
        extra = "".join(random.choices(string.ascii_letters + string.digits, k=3))  # –°–ª—É—á–∞–π–Ω—ã–µ –±—É–∫–≤—ã –∏ —Ü–∏—Ñ—Ä—ã
        special = random.choice(special_chars)  # –û–¥–∏–Ω —Å–ø–µ—Ü—Å–∏–º–≤–æ–ª
        
        password = f"{base}{extra}{special}"
        passwords.append(password)

    return passwords

# –í–≤–æ–¥ –¥–∞–Ω–Ω—ã—Ö
full_name = input("–í–≤–µ–¥–∏—Ç–µ –§–ò–û: ")
birth_date = input("–í–≤–µ–¥–∏—Ç–µ –¥–∞—Ç—É —Ä–æ–∂–¥–µ–Ω–∏—è (–¥–¥.–º–º.–≥–≥–≥–≥): ")

# –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –ø–∞—Ä–æ–ª–µ–π
generated_passwords = generate_passwords(full_name, birth_date)

# –í—ã–≤–æ–¥ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤
print("\nüîí –°–≥–µ–Ω–µ—Ä–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ –ø–∞—Ä–æ–ª–∏:")
for idx, pwd in enumerate(generated_passwords, 1):
    print(f"{idx}. {pwd}")
